  setFeedback(feedback.filter((e) => e.id !== id));\n    }\n  };\n\n  // Update feedback item\n  const updateFeedback = async (id, updItem) => {\n    const response = await fetch(`/feedback/${id}`, {\n      method: \"PUT\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(updItem),\n    });\n    const data = await response.json();\n\n    setFeedback(\n      feedback.map((item) =>\n        item.id === id\n          ? {\n              ...item,\n              ...data,\n            }\n          : item\n      )\n    );\n  };\n\n  // Set item to be updated\n  const editFeedback = (item) => {\n    setFeedbackEdit({\n      item,\n      edit: true,\n    });\n  };\n\n  const addFeedback = async (newFeedback) => {\n    const response = await fetch(\"/feedback\", {\n      method: \"POST\",\n      headers: { \"Content-Type\": \"application/json\" },\n      body: JSON.stringify(newFeedback),\n    });\n\n    const data = await response.json();\n    setFeedback([data, ...feedback]);\n  };\n\n  return (\n    <FeedbackContext.Provider\n      value={{\n        feedback,\n        feedbackEdit,\n        isLoading,\n        deleteFeedback,\n        addFeedback,\n        editFeedback,\n        updateFeedback,\n      }}\n    >\n      {children}\n    </FeedbackContext.Provider>\n  );\n};\n\nexport default FeedbackContext;\n","/Users/akrik/Desktop/udemy-react-front-to-back/feedback-app/src/components/FeedbackList.jsx",[],[],"/Users/akrik/Desktop/udemy-react-front-to-back/feedback-app/src/components/FeedbackForm.jsx",[],[],"/Users/akrik/Desktop/udemy-react-front-to-back/feedback-app/src/components/Header.jsx",[],[],"/Users/akrik/Desktop/udemy-react-front-to-back/feedback-app/src/components/AboutLink.jsx",[],[],"/Users/akrik/Desktop/udemy-react-front-to-back/feedback-app/src/components/FeedbackStats.jsx",[],[],"/Users/akrik/Desktop/udemy-react-front-to-back/feedback-app/src/pages/AboutPage.jsx",[],[],"/Users/akrik/Desktop/udemy-react-fron